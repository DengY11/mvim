Checks: "-*,bugprone-*,readability-*,modernize-use-nullptr,modernize-use-using,modernize-avoid-c-arrays,performance-*"
WarningsAsErrors: ""
HeaderFilterRegex: "src/.*"
FormatStyle: "file"
CheckOptions:
  - key: readability-identifier-naming.VariableCase
    value: lower_case
  - key: readability-identifier-naming.FunctionCase
    value: lowerCamelCase
  - key: readability-identifier-naming.ClassCase
    value: PascalCase
